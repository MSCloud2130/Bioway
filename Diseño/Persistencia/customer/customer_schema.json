{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "_id": {
                "$oid": "613e3070d2acf338c07a2d2b"
            },
            "email": "pedro.perez@gmail.com",
            "name": "Pedro Perez",
            "age": 25,
            "picture": "",
            "description": "Me gusta viajar.",
            "_class": "javeriana.ms.customer.entities.Customer"
        }
    ],
    "required": [
        "_id",
        "email",
        "name",
        "age",
        "picture",
        "description",
        "_class"
    ],
    "properties": {
        "_id": {
            "$id": "#/properties/_id",
            "type": "object",
            "title": "The _id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "$oid": "613e3070d2acf338c07a2d2b"
                }
            ],
            "required": [
                "$oid"
            ],
            "properties": {
                "$oid": {
                    "$id": "#/properties/_id/properties/%24oid",
                    "type": "string",
                    "title": "The $oid schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "613e3070d2acf338c07a2d2b"
                    ]
                }
            },
            "additionalProperties": true
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "pedro.perez@gmail.com"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Pedro Perez"
            ]
        },
        "age": {
            "$id": "#/properties/age",
            "type": "integer",
            "title": "The age schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                25
            ]
        },
        "picture": {
            "$id": "#/properties/picture",
            "type": "string",
            "title": "The picture schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                ""
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Me gusta viajar."
            ]
        },
        "_class": {
            "$id": "#/properties/_class",
            "type": "string",
            "title": "The _class schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "javeriana.ms.customer.entities.Customer"
            ]
        }
    },
    "additionalProperties": true
}