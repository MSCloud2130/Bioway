{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "_id": "8c11d34a-61cb-4b40-b269-a6864a2aeb6b",
            "name": "Pedro tully",
            "email": "pedtul@supplier.com",
            "age": 30,
            "picture": "picture",
            "description": "description",
            "_class": "com.omega.bioway.customers.crosscutting.entities.Customer"
        }
    ],
    "required": [
        "_id",
        "name",
        "email",
        "age",
        "picture",
        "description",
        "_class"
    ],
    "properties": {
        "_id": {
            "$id": "#/properties/_id",
            "type": "string",
            "title": "The _id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "8c11d34a-61cb-4b40-b269-a6864a2aeb6b"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Pedro tully"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "pedtul@supplier.com"
            ]
        },
        "age": {
            "$id": "#/properties/age",
            "type": "integer",
            "title": "The age schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                30
            ]
        },
        "picture": {
            "$id": "#/properties/picture",
            "type": "string",
            "title": "The picture schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "picture"
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "description"
            ]
        },
        "_class": {
            "$id": "#/properties/_class",
            "type": "string",
            "title": "The _class schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "com.omega.bioway.customers.crosscutting.entities.Customer"
            ]
        }
    },
    "additionalProperties": true
}