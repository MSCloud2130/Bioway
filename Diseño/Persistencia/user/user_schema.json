{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "User schema",
    "description": "Schema usado para la persistencia de los productos en el microservicio identity",
    "default": {},
    "examples": [
        {
            "_id": "60f3780b-7489-4b78-883e-96c5bdad9455",
            "email": "pedro.perez@gmail.com",
            "password": "Qwerty1234!",
            "type": "CUSTOMER",
            "_class": "com.omega.bioway.identity.crosscutting.entities.User"
        }
    ],
    "required": [
        "_id",
        "email",
        "password",
        "type",
        "_class"
    ],
    "properties": {
        "_id": {
            "$id": "#/properties/_id",
            "type": "string",
            "title": "The _id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "60f3780b-7489-4b78-883e-96c5bdad9455"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "pedro.perez@gmail.com"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Qwerty1234!"
            ]
        },
        "type": {
            "$id": "#/properties/type",
            "type": "string",
            "title": "The type schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "CUSTOMER"
            ]
        },
        "_class": {
            "$id": "#/properties/_class",
            "type": "string",
            "title": "The _class schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "com.omega.bioway.identity.crosscutting.entities.User"
            ]
        }
    },
    "additionalProperties": true
}